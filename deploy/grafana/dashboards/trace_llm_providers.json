          "legendFormat": "{{provider}} P50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(trace_chat_call_duration_seconds_bucket[5m])) by (le, provider))",
          "legendFormat": "{{provider}} P95"
        },
