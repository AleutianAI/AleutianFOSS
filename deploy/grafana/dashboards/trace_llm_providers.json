      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(trace_llm_errors_total[5m])) by (provider, error_type)",
          "legendFormat": "{{provider}} {{error_type}}"
        }
      ]
    },
    {
      "title": "Chat Client Latency P50/P95/P99",
      "type": "timeseries",
